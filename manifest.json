{
  "manifest_version": 2,

  "name": "Text to speech",
  "description": "This extension will speak the selected text from a web page.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
